{
  "expo": {
    "name": "minglee",
    "slug": "minglee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MingleeLogo.png",
    "scheme": "minglee",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MingleeLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.theshafqatullah.minglee"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SplashLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SplashLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c394d429-9d47-433e-a4da-88bc0ba2ffaf"
      }
    }
  }
}
